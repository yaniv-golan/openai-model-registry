{
    "type": "object",
    "properties": {
        "proceed": {
            "type": "boolean"
        },
        "confidence": {
            "type": "string",
            "enum": [
                "high",
                "medium",
                "low"
            ]
        },
        "reasons": {
            "type": "string"
        },
        "last_update_date": {
            "type": [
                "string",
                "null"
            ],
            "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
        },
        "updated_models": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "model": {
                        "type": "string"
                    },
                    "evidence_url": {
                        "type": "string",
                        "pattern": "^https://"
                    },
                    "old_pricing": {
                        "type": "object",
                        "properties": {
                            "scheme": {
                                "type": "string"
                            },
                            "unit": {
                                "type": "string"
                            },
                            "input_cost_per_unit": {
                                "type": "number"
                            },
                            "output_cost_per_unit": {
                                "type": "number"
                            },
                            "currency": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "scheme",
                            "unit",
                            "input_cost_per_unit",
                            "output_cost_per_unit",
                            "currency"
                        ],
                        "additionalProperties": false
                    },
                    "new_pricing": {
                        "type": "object",
                        "properties": {
                            "scheme": {
                                "type": "string"
                            },
                            "unit": {
                                "type": "string"
                            },
                            "input_cost_per_unit": {
                                "type": "number"
                            },
                            "output_cost_per_unit": {
                                "type": "number"
                            },
                            "currency": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "scheme",
                            "unit",
                            "input_cost_per_unit",
                            "output_cost_per_unit",
                            "currency"
                        ],
                        "additionalProperties": false
                    }
                },
                "required": [
                    "model",
                    "evidence_url",
                    "old_pricing",
                    "new_pricing"
                ],
                "additionalProperties": false
            }
        },
        "new_models": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "model": {
                        "type": "string"
                    },
                    "announced_date": {
                        "type": "string",
                        "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
                    },
                    "evidence_url": {
                        "type": "string",
                        "pattern": "^https://"
                    },
                    "initial_pricing": {
                        "type": "object",
                        "properties": {
                            "scheme": {
                                "type": "string"
                            },
                            "unit": {
                                "type": "string"
                            },
                            "input_cost_per_unit": {
                                "type": "number"
                            },
                            "output_cost_per_unit": {
                                "type": "number"
                            },
                            "currency": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "scheme",
                            "unit",
                            "input_cost_per_unit",
                            "output_cost_per_unit",
                            "currency"
                        ],
                        "additionalProperties": false
                    }
                },
                "required": [
                    "model",
                    "announced_date",
                    "evidence_url"
                ],
                "additionalProperties": false
            }
        },
        "rumors": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "model": {
                        "type": "string"
                    },
                    "sources": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "pattern": "^https://"
                        },
                        "minItems": 1,
                        "maxItems": 8
                    }
                },
                "required": [
                    "model",
                    "sources"
                ],
                "additionalProperties": false
            }
        }
    },
    "required": [
        "proceed"
    ],
    "additionalProperties": false
}
